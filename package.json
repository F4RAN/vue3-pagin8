{
  "name": "vue3-pagin8",
  "version": "1.0.0",
  "repository": "https://github.com/F4RAN/vue3-pagin8",
  "bugs": {
    "url": "https://github.com/F4RAN/vue3-pagin8/issues",
    "email": "f4ran@vitalize.dev"
  },
  "license": "MIT",
  "description": "Vue pagination using beautiful and simple methods",
  "main": "dist/vue3-pagin8.ssr.js",
  "browser": "dist/vue3-pagin8.esm.js",
  "module": "dist/vue3-pagin8.esm.js",
  "unpkg": "dist/vue3-pagin8.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "README.md"
  ],
  "author": "F4RAN <f4ran@vitalize.dev> (https://github.com/F4RAN)",
  "keywords": [
    "vue",
    "vue-paginate",
    "vue-pagination",
    "paginate",
    "pagination",
    "simple paginate",
    "beautiful paginate"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.0.11",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "postcss": "^8.2.10",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^3.0.5"
  },
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "engines": {
    "node": ">=12"
  }
}